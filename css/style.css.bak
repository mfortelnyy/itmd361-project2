#navbar{
  background-color: CornflowerBlue;
  overflow: hidden; 
}

#nav-container{
  
  /*width: 100%;
  position: relative;
  */
  text-align: center;
  
}
#navbar ul {
  list-style-type: none;
  margin: 0; 
  padding: 0; 
  display: inline-block;
  vertical-align: top;
}
#navbar ul li{
  float: left;
  /*position: relative; 
  display: block;*/
  font-family: 'Teko', sans-serif;
  font-size: 1.6em;
}

#navbar ul li a{
  display: block;
  padding: 10px 20px;
  color: NavajoWhite;
  text-decoration: none;
}

#navbar ul li a:hover{
  text-decoration: underline;
  background-color: DarkOrchid;
}

h1{
	font-weight: 700;
}

