#navbar{
  background-color: CornflowerBlue;
  overflow: hidden; 
  width:100%;
}

#nav-container{
  
  width: 100%;
  position: relative;
  
  text-align: center;
  margin:0;
  padding:0;
  
}
#navbar ul {
  list-style-type: none;
  margin: 0; 
  padding: 0; 
  display: inline-block;
  vertical-align: top;
}
#navbar ul li{
  float: left;
  /*position: relative; 
  display: block;*/
  font-family: 'Teko', sans-serif;
  font-size: 1.6em;
}

#navbar ul li a{
  display: block;
  padding: 10px 20px;
  color: NavajoWhite;
  text-decoration: none;
}

#navbar ul li a:hover{
  text-decoration: underline;
  background-color: DarkOrchid;
}

